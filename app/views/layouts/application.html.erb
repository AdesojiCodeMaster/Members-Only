<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    
  </head>

  <body>
 
  <% if current_member %> 
   <%= button_to "Sign out", destroy_member_session_path, :method => :delete, class: "destroy_member_session" %>
   <%else%>
   <div class='nav'>
    <nav>
   <button><%= link_to "Sign up", new_member_registration_path,  class: "new_member_registration" %></button>
   <button><%= link_to "Login", new_member_session_path,  class: "new_member_session" %></button>
    </nav>
    </div>
   
<% end %>  

  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
